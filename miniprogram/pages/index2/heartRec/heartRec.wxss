/* 最大页面的底色 */
.page{
  width: 100%;
  height: 100%;
  background-color: #ffffff;
}
/* 标题 */
.topcontainer{
  width:100%;
  background-color:#ffffff;
  border-radius: 2%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 7%;
}
/*标题1*/
.case_title{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50%;
  height: 100%;
  background-color: #fff3eb;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  border-radius: 30rpx;
  color: #ff9933;
  font-size: 60rpx;
  font-weight: bold;
}
.reccontainer{
  width:100%;
  height:1200rpx;
  background-color:#fff3eb;
  border-radius: 2%;
  display: flex;
  flex-direction: column;
}
.reccontainer1{
  width: 100%;
  height: 20%;
  background-color:#000000;
}
.reccontainer2{
  width: 80%;
  height: 100%;
  background-color:#434f50;
}